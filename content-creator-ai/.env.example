# Server Configuration
PORT=3000
NODE_ENV=development
API_KEY=your-webhook-api-key-here

# AI Provider API Keys
OPENAI_API_KEY=sk-your-openai-key
GOOGLE_API_KEY=your-google-ai-studio-key
ZAI_API_KEY=your-z-ai-api-key
ANTHROPIC_API_KEY=your-anthropic-key
PERPLEXITY_API_KEY=your-perplexity-key

# LocalAI Configuration (for self-hosted/local models)
LOCALAI_URL=http://localhost:8080
LOCALAI_ENABLED=false

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379
REDIS_ENABLED=false

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Default AI Provider (openai, google, localai, zai, anthropic, perplexity)
DEFAULT_PROVIDER=google

# Google AI Configuration
GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id
GOOGLE_MAPS_API_KEY=your-maps-api-key

# Z.AI Configuration
ZAI_API_ENDPOINT=https://api.z.ai/api/paas/v4/chat/completions
ZAI_MODEL=glm-4.6

# Cost Tracking
TRACK_COSTS=true

# Logging
LOG_LEVEL=info
