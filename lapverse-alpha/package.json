{
  "name": "lapverse-alpha",
  "version": "0.1.0",
  "description": "The $100K 14-Day Challenge",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "deploy:alpha": "ts-node scripts/deploy-alpha.ts",
    "test": "vitest run",
    "test:phase1": "vitest run --testNamePattern=Phase1",
    "test:phase2": "vitest run --testNamePattern=Phase2",
    "test:integration": "vitest run --testNamePattern=Integration"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "ollama": "^0.6.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@vitest/ui": "^1.0.0",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}