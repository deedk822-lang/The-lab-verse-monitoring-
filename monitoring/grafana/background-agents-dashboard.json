        "type": "graph",
        "targets": [
          {
            "expr": "rate(zapier_webhook_retries_total[5m])",
            "legendFormat": "{{agent_id}} - {{action}} - Attempt {{retry_attempt}}",
            "refId": "A"
          }
        ],
        "xAxis": {
          "show": true
