      },
      {
        "id": 15,
        "title": "Background Agent Utilization",
        "type": "singlestat",
        "targets": [
          {
            "expr": "avg(background_agent_utilization_percent)",
            "legendFormat": "Average Utilization",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "50,80",
        "colorBackground": true,
        "colorValue": false,
