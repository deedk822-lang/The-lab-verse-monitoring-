{
  "name": "the-lab-verse-monitoring",
  "version": "2.0.0",
  "description": "AI and payment agents monitoring system for WhatsApp, M-Pesa, Stability AI, and ElevenLabs",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "build": "echo 'Express server - no build step'",
    "test": "jest --runInBand --coverage --verbose --testTimeout=10000",
    "test:watch": "jest --watch",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky install",
    "clean": "rimraf node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install",
    "postinstall": "if [ -d 'crewai' ]; then pip3 install -r crewai/requirements.txt; else echo 'crewai directory not found, skipping pip install'; fi",
    "health": "node -e \"require('http').get('http://localhost:3000/health', r=>{console.log('✅ Health check passed'); r.on('error', e=>{console.error('❌ Health check failed:', e); process.exit(1)});}).on('error', e=>{console.error('❌ Health check failed:', e); process.exit(1)});\""
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@mistralai/mistralai": "^1.10.0",
    "@vercel/postgres": "^0.10.0",
    "@vercel/speed-insights": "^1.0.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "lucide-react": "^0.555.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.63.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.5.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.14.0",
    "eslint": "^8.52.0",
    "globals": "^15.11.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/deedk822-lang/The-lab-verse-monitoring-.git"
  },
  "keywords": [
    "ai",
    "monitoring",
    "whatsapp",
    "mpesa",
    "agents"
  ],
  "author": "deedk822-lang",
  "license": "MIT"
}
