{
  "name": "the-lab-verse-monitoring",
  "version": "2.0.0",
  "description": "AI and payment agents monitoring system for WhatsApp, M-Pesa, Stability AI, and ElevenLabs",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "jest --runInBand --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky || true",
    "clean": "rimraf node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "axios": "^1.7.7",
    "prom-client": "^15.1.3",
    "sqlite3": "^5.1.7",
    "openai": "^4.63.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deedk822-lang/The-lab-verse-monitoring-.git"
  },
  "keywords": [
    "ai",
    "monitoring",
    "whatsapp",
    "mpesa",
    "agents"
  ],
  "author": "deedk822-lang",
  "license": "MIT"
}
