{
  "name": "lab-verse-monitoring",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:ai": "node test-groq-integration.js",
    "test:ai-sdk": "NODE_OPTIONS=--experimental-vm-modules npx jest test/ai-sdk.test.js",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "test:analyze": "NODE_OPTIONS=--experimental-vm-modules npx jest --listTests && node scripts/analyze-tests.js",
    "test:performance": "NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles --logHeapUsage",
    "lint": "eslint src test --ext .js",
    "lint:fix": "eslint src test --ext .js --fix",
    "prepare": "husky install || true",
    "precommit": "lint-staged",
    "clean:test": "rm -rf .jest-cache coverage test-results"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^2.0.27",
    "@ai-sdk/openai": "^1.0.0",
    "@automattic/mcp-wpcom-remote": "0.2.16",
    "@google/generative-ai": "^0.24.1",
    "@octokit/auth-app": "^6.0.1",
    "@octokit/core": "^5.0.2",
    "@octokit/rest": "^20.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation-express": "^0.57.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@sentry/node": "^10.25.0",
    "@sentry/tracing": "^7.120.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.0.0",
    "axios": "^1.7.2",
    "cluster": "^0.7.7",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.0",
    "groq-sdk": "^0.34.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "openai": "^6.8.1",
    "os": "^0.1.2",
    "perplexity": "^1.0.1",
    "pino": "^10.1.0",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.3",
    "socket.io": "^4.7.0",
    "winston": "^3.18.3",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@jest/globals": "^30.2.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
