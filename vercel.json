{
  "buildCommand": "pip install -r requirements.txt && mypy --strict rainmaker_orchestrator/ api/",
  "outputDirectory": ".",
  "framework": "fastapi",
  "env": {
    "ENVIRONMENT": "production",
    "CI": "true",
    "OPENLIT_OTLP_ENDPOINT": "@OPENLIT_OTLP_ENDPOINT",
    "ZAI_API_KEY": "@ZAI_API_KEY",
    "MISTRAL_API_KEY": "@MISTRAL_API_KEY",
    "REDIS_URL": "@REDIS_URL"
  },
  "routes": [
    {
      "src": "/health",
      "dest": "api/server.py"
    },
    {
      "src": "/execute",
      "methods": ["POST"],
      "dest": "api/server.py"
    },
    {
      "src": "/webhook/hubspot",
      "methods": ["POST"],
      "dest": "api/server.py"
    }
  ],
  "functions": {
    "api/server.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
